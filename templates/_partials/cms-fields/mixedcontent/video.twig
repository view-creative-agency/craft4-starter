{% import '_macros/helpers.twig' as helpers -%}
<div
	class="mixedContent_video"
	id="block-{{ block.id }}"
	data-scroll-reveal
>
	{{ helpers.videoEmbed2({
		videoUrl:   block.youtubeVimeoVideoUrl,
		autoplay:   block.autoplay,
		loop:       block.loopVideo,
		minimalUi:  block.minimalUi,
		background: null,
		aspectRatio: block.aspectRatio
	}) }}
</div>
